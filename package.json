{
  "name": "junior-dev-taks",
  "description": "Test applcation for junior developers",
  "author": "KODA Bots",
  "private": true,
  "scripts": {
    "front:develop": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot",
    "front:build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "server:build": "tsc",
    "server:watch": "tsc --watch",
    "server:start": "tsc && node dist/server/server.js",
    "server:test": "tsc; mocha -R spec dist/tests/*.spec.js"
  },
  "dependencies": {
    "@types/express": "^4.17.3",
    "@types/mocha": "^7.0.2",
    "body-parser": "^1.4.3",
    "express": "^4.4.5",
    "typescript": "^3.8.3",
    "vue": "^2.0.1"
  },
  "devDependencies": {
    "assert": "^2.0.0",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^3.0.0",
    "css-loader": "^3.4.2",
    "file-loader": "^0.9.0",
    "mocha": "^7.1.0",
    "sinon": "^9.0.0",
    "supertest": "^4.0.2",
    "vue-loader": "^9.7.0",
    "vue-resource": "^1.0.3",
    "vue-router": "^2.0.1",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.0"
  }
}
